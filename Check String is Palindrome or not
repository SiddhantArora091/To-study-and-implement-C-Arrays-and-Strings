#include<iostream>
using namespace std;
int main() {
    string str,reversed;
    cout <<"Enter a string: ";
    cin >> str;
    int n=str.length();
    for(int i=n-1;i>=0;i--) {
        reversed+=str[i];
    }
    if(str==reversed){
        cout<<"The string is a palindrome";
    } else{
        cout<<"The string is not a palindrome";
    }
    return 0;
}
/*Output:-
Enter a string: b
The string is a palindrome
*/
